// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
 model student{
  mail_id String @unique @id
  reg_no String @unique
  first_name String
  last_name String
 }

 model staffs{
  staff_id String @db.Uuid @id @default(uuid())
  first_name String
  last_name String
  mail_id String @unique
 }
